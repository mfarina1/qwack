    "def update_circuit(circuit, gate_name, qubit_no):\n",
    "    \n",
    "    if gate_name == 'x':\n",
    "        circuit.x(qubit_no)\n",
    "    \n",
    "    if gate_name == 'y':\n",
    "        circuit.y(qubit_no)\n",
    "    \n",
    "    if gate_name == 'z':\n",
    "        circuit.z(qubit_no)\n",
    "        \n",
    "    if gate_name == 'h':\n",
    "        circuit.h(qubit_no)\n",
    "        \n",
    "    if gate_name == 'h':\n",
    "        circuit.h(qubit_no)\n",
    "        \n",
    "    if gate_name == 'measure':\n",
    "        pass\n",
    "        #circuit.measure(qubit_no)\n",
    "        \n",
    "    if gate_name == 's':\n",
    "        circuit.s(qubit_no)\n",
    "    \n",
    "    if gate_name == 't':\n",
    "        circuit.t(qubit_no)\n",
    "    \n",
    "    circuit.draw(output='mpl',filename='test.png')\n",
    "\n",
    "    circ_img = pygame.image.load('test.png')\n",
    "\n",
    "    return circuit, circ_img\n",
    "    #enumerate through user actions instead of all if statements?\n",
    "    \n",
    "    #user selects qubit, then selects action on qubit"
